<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.demo.mall.dao.OrderItemMapper">

    <select id="listByOrderNumber" resultType="com.demo.mall.bean.model.OrderItem">
        SELECT *
        FROM tz_order_item
        WHERE order_number = #{orderNumber}
    </select>
</mapper>